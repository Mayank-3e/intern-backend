{
    "version": 1,
    "name": "CRUD-APP-backend",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js",
        "methods": [
          "GET",
          "POST"
        ]
      }
    ]
}